	MVI A,0DH
	MVI E,00H
	SIM
LOOP_C:
	MOV A,D
	ANI 0FH
	ORA E
	CMA
	STA 3000H
	INR D
	DI
	LXI B,0064H
	CALL DELB
	EI
	JMP LOOP_C
INTR_ROUTINE:
	MVI B,08H
	MVI C,00H
	LDA 2000H
	MOV E,A
LEDS:	
	MOV A,E
	RRC
	JNC SKIP
	INR C
SKIP:
	DCR B
	MOV E,A
	MOV A,B
	CPI 00H
	JNZ LEDS
	MOV A,C
	RAL
	RAL
	RAL
	RAL
	MOV E,A
	JMP LOOP_C
END
